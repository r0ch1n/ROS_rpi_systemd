[Unit]
Description=Launcher for the ROS master, parameter server and rosout logging node
After=network.target

[Service]
User=mcp
EnvironmentFile=/etc/default/roscore
ExecStart=/opt/ros/kinetic/bin/roslaunch raspicam_node ~/ros_catkin_ws/src/raspicam_node/launch/camerav2_1280x960.launch
Restart=on-abort

[Install]
WantedBy=multi-user.target
